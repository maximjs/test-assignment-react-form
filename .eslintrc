{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "rules": {
    "max-len": ["error", { "code": 200 }],
    "no-console": 0,
    "react/prop-types": 0,
    "class-methods-use-this": 0
  },
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ]
}
